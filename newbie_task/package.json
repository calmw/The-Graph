{
  "name": "statistics-subgraph",
  "license": "MIT",
  "scripts": {
    "build-dev": "rm -rf subgraph.yaml && cp subgraph-dev.yaml subgraph.yaml && graph codegen && graph build",
    "build-main": "rm -rf subgraph.yaml && cp subgraph-main.yaml subgraph.yaml graph codegen && graph build",
    "build-prod": "graph codegen && graph build",
    "create": "graph create into-newbie-task --node http://127.0.0.1:8020",
    "deploy": "graph deploy into-newbie-task --ipfs http://localhost:5001 --node http://127.0.0.1:8020/",
    "remove": "graph remove into-newbie-task --node http://localhost:8020/"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.49.0",
    "@graphprotocol/graph-ts": "^0.29.1"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0"
  }
}
